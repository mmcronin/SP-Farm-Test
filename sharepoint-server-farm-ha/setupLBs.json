{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "type": "string"
    }
    },
    "resources":[
      {
        "apiVersion": "2015-06-15",
        "name": "[parameters('spCALBName')]",
        "type": "Microsoft.Network/loadBalancers",
        "location": "[parameters('location')]",
        "properties": {
          "frontendIPConfigurations": [
            {
              "name": "[parameters('spCALBFE')]",
              "properties": {
                "publicIPAddress": {
                  "id": "[parameters('spCAResourceId')]"
                }
              }
            }
          ],
          "backendAddressPools": [
            {
              "name": "[parameters('spCALBBE')]"

            }
          ],
          "inboundNatRules": [
            {
              "name": "[parameters('spCANAT')]",
              "properties": {
                "frontendIPConfiguration": {
                  "id": "[parameters('spCAlbFEConfigID')]"
                },
                "protocol": "tcp",
                "frontendPort": 80,
                "backendPort": 80,
                "enableFloatingIP": false
              }
            }
          ]
        }
      },
      {
        "apiVersion": "2015-06-15",
        "name": "[parameters('spWebLBName')]",
        "type": "Microsoft.Network/loadBalancers",
        "location": "[parameters('location')]",
        "properties": {
          "frontendIPConfigurations": [
            {
              "name": "[parameters('spWebLBFE')]",
              "properties": {
                "publicIPAddress": {
                  "id": "[parameters('spWebResourceId')]"
                }
              }
            }
          ],
          "backendAddressPools": [
            {
              "name": "[parameters('spWebLBBE')]"
            }
          ],
          "loadBalancingRules": [
            {
              "name": "[parameters('spWebLB')]",
              "properties": {
                "frontendIPConfiguration": {
                  "id": "[parameters('spWebLBFEConfigID')]"
                },
                "backendAddressPool": {
                  "id": "[parameters('spWebBEAddressPoolID')]"
                },
                "probe": {
                  "id": "[parameters('spWebProbeID')]"
                },
                "protocol": "tcp",
                "frontendPort": 80,
                "backendPort": 80,
                "enableFloatingIP": false
              }
            }
          ],
          "probes": [
            {
              "name": "[parameters('spWebProbe')]",
              "properties": {
                "protocol": "http",
                "port": 8088,
                "intervalInSeconds": "15",
                "numberOfProbes": "5",
                "requestPath": "/iisstart.htm"
              }
            }
          ]
        }
      },
      {
        "apiVersion": "2015-06-15",
        "name": "[parameters('sqlLBName')]",
        "type": "Microsoft.Network/loadBalancers",
        "location": "[parameters('location')]",
        "properties": {
          "frontendIPConfigurations": [
            {
              "name": "[parameters('sqlLBFE')]",
              "properties": {
                "privateIPAllocationMethod": "Static",
                "privateIPAddress": "[parameters('sqlLBIPAddress')]",
                "subnet": {
                  "id": "[parameters('staticSubnetRef')]"
                }
              }
            }
          ],
          "backendAddressPools": [
            {
              "name": "[parameters('sqlLBBE')]"
            }
          ]
        }
      }
    ],
    "spCALBBE": {
      "type": "string"
    },
    "spCALBFE": {
      "type": "string"
    },
    "spCAlbFEConfigID": {
      "type": "string"
    },
    "spCALBName": {
      "type": "string"
    },
    "spCANAT": {
      "type": "string"
    },
    "spCAResourceId": {
      "type": "string"
    },
    "spWebBEAddressPoolID": {
      "type": "string"
    },
    "spWebLB": {
      "type": "string"
    },
    "spWebLBBE": {
      "type": "string"
    },
    "spWebLBFE": {
      "type": "string"
    },
    "spWebLBFEConfigID": {
      "type": "string"
    },
    "spWebLBName": {
      "type": "string"
    },
    "spWebProbe": {
      "type": "string"
    },
    "spWebProbeID": {
      "type": "string"
    },
    "spWebResourceId": {
      "type": "string"
    },
    "sqlLBBE": {
      "type": "string"
    },
    "sqlLBFE": {
      "type": "string"
    },
    "sqllbFEConfigID": {
      "type": "string"
    },
    "sqlLBIPAddress": {
      "type": "string"
    },
    "sqlLBName": {
      "type": "string"
    },
    "staticSubnetRef": {
      "type": "string"
    }
  }
